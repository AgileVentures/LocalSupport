{
  "name": "LocalSupport",
  "version": "2.0.0",
  "description": "A directory of local support services and volunteer opportunities",
  "main": "application.js",
  "engines": {
    "node": ">=8.9.4",
    "yarn": ">= 1.0.0"
  },
  "engineStrict": true,
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "scripts": {
    "requirements-check": "node check-version.js",
    "test": "bundle exec rake cucumber && bundle exec rake rspec && bundle exec rake jasmine:ci",
    "postinstall": "node -e \"try { require('fs').symlinkSync(require('path').resolve('node_modules/@bower_components'), 'vendor/assets/bower_components', 'junction') } catch (e) { }\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AgileVentures/LocalSupport.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AgileVentures/LocalSupport/issues"
  },
  "homepage": "https://github.com/AgileVentures/LocalSupport",
  "dependencies": {
    "bower": "^1.3.12",
    "semver": "^5.5.0",
    "@bower_components/google-infobox": "PepijnSenders/google-infobox#*"
  }
}
